{
  "name": "unfolded-snippets",
  "displayName": "Unfolded Snippets",
  "description": "VSCode Snippets for Unfolded Map and Data SDKs",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Data Science",
    "Education",
    "Notebooks",
    "Snippets",
    "Visualization"
  ],
  "keywords": [
    "unfolded map sdk",
    "unfolded data sdk",
    "unfolded map snippets",
    "unfolded data snippets",
    "unfolded map html code snippets",
    "unfolded map notebooks"
  ],
  "icon": "resources/icons/unfolded-logo.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak",
  "contributors": [
    "Taras Novak"
  ],
  "license": "Apache-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/unfolded-map-snippets"
  },
  "bugs": "https://github.com/RandomFractals/unfolded-map-snippets/issues",
  "homepage": "https://github.com/RandomFractals/unfolded-map-snippets/README.md",
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:unfolded.snippets.notebook.examples"
  ],
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unfolded.snippets.notebook.examples",
        "title": "Notebook Examples",
        "category": "Unfolded",
        "icon": "$(notebook)"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "python",
        "path": "./snippets/python.code-snippets"
      }
    ]
  },
  "extensionDependencies": [
    "RandomFractalsInc.snippets-viewer"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run compile:extension",
    "compile:extension": "tsc -b",
    "compile:renderer": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "concurrently -r \"npm:watch:*\"",
    "watch:extension": "tsc -b --watch",
    "watch:renderer": "webpack --mode development --watch",
    "dev": "concurrently -r npm:watch:extension",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.4",
    "@types/vscode": "^1.61.0",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "vscode-test": "^1.6.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {}
}
